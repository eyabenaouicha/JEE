<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  
  <display-name>Clubix</display-name>
  <servlet>
    <servlet-name>ServletAuthentification</servlet-name>
    <servlet-class>web.ServletAuthentification</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletAuthentification</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>ServletShowTrace</servlet-name>
    <jsp-file>/ShowTrace.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletShowTrace</servlet-name>
    <url-pattern>/historique/*</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>ServletEnregistrement</servlet-name>
    <jsp-file>/SaveOperation.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletEnregistrement</servlet-name>
    <url-pattern>/saveOperation/*</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>ServletShowTraceByDate</servlet-name>
    <jsp-file>/ShowTraceByDate.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletShowTraceByDate</servlet-name>
    <url-pattern>/DailyTrace/*</url-pattern>
  </servlet-mapping>
</web-app>